{
  "name": "custom-stock-e-b",
  "vendor": "vtexarg",
  "version": "0.0.2",
  "title": "Custom Stock Export Back",
  "description": "Custom Stock Export Back",
  "mustUpdateAt": "2018-01-04",
  "categories": [],
  "dependencies": {
    "vtex.file-manager-graphql": "0.x",
    "vtex.file-manager": "0.x"
  },
  "builders": {
    "node": "6.x",
    "docs": "0.x"
  },
  "billingOptions": {
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "type": "free",
    "availableCountries": [
      "*"
    ]
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/mail-service/pvt/*"
      }
    },
    {
      "name": "ProdutoForm.aspx"
    },
    {
      "name": "Sku.aspx"
    },
    {
      "name": "LogisticsAdmin"
    },
    {
      "name": "send-message"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "httpstat.us",
        "path": "*"
      }
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "vtex.file-manager:file-manager-read-write"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema",
  "settingsSchema": {
    "title": "VTEX Proxy Mailing Flow",
    "type": "object",
    "properties": {
      "customStockExportTemplate": {
        "title": "Custom Stock Export Mailing Template Name",
        "description": "The name at the end of the message-center template url.",
        "type": "string"
      }
    }
  }
}
